    <tag:stardog:studio:label> "healthWorker" .

fse:doctor a owl:Class ;
    rdfs:label "doctor" ;
    rdfs:subClassOf fse:healthWorker ;
    <tag:stardog:studio:label> "doctor" .

fse:nurse a owl:Class ;
    rdfs:label "nurse" ;
